<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:minHeight="44dip"
    android:paddingLeft="10dip" >
    
    <ImageView
        android:id="@+id/imagenCeldaTipoContacto"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="5dp" />
    
    <com.infomovil.infomovil.gui.common.TextWithFont
        android:id="@+id/labelNombreTipoContacto"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imagenCeldaTipoContacto"
        android:layout_marginLeft="10dp"
        android:textColor="@color/colorFuenteVerde" 
        android:textSize="19sp" />
    

</RelativeLayout>
