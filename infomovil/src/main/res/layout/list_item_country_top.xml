<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:id="@+id/itemContainerCountry"
    style="@style/list_item_top" >
    
    <com.infomovil.infomovil.gui.common.TextWithFont 
        android:id="@+id/labelPais3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="10dp"
        android:textColor="@color/colorFuenteVerde"
        android:textSize="19sp" />
    
    <com.infomovil.infomovil.gui.common.TextWithFont 
        android:id="@+id/labelCodigoPais3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="5dp"
        android:textColor="@color/colorFuenteVerde"
        android:textSize="19sp"
        android:gravity="right" />
    

</RelativeLayout>